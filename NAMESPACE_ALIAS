namespace path = MyComp.MyProject.MyApp.IPL
{
    class Dept
    {
        public void show()
        {
            Console.WriteLine("This is from dept class MyNameSpace namespace");
        }

        public void getFun()
        {
            Console.WriteLine("getFun from dept class MyNameSpace namespace");
        }
    }
}

class MultipleNameSpace
{
    public static void Main(string[] args)
    {
        path.Dept d = new path.Dept();
        d.show();
        d.getFun();
    }
}
