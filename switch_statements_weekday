using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        int day = 4;

        string dayName = day switch
        {
            1 => "Monday",
            2 => "Tuesday",
            3 => "Wednesday",
            4 => "Thursday",
            5 => "Friday",
            6 => "Saturday",
            7 => "Sunday",
            _ => "Invalid Input"
        };

        Console.WriteLine(dayName);
    }
}
